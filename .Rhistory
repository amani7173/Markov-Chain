library(lpSolve)
obj_coeff <- c(13, 23, 30)
# Constraint Coefficients
constraints <- matrix(
c(5,15,4,
4,4,10,
35,20,15,
5,10,20
), nrow=4, byrow=TRUE
)
direction_c <- c("<=",
"<=",
"<=",
"<=")
resources <- c(530,
444,
1365,
840
)
solve_lp <- lp(
"max",
obj_coeff,
constraints,
direction_c,
resources
)
# Get the max profit
solve_lp$objval
# Best values
solve_lp$solution
